{
  "name": "scae_temp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:live": "cross-env VITE_PROXY_TARGET=https://scae.pages.dev npm run dev",
    "dev:local": "cross-env VITE_PROXY_TARGET=http://localhost:8788 npm run dev",
    "dev:api:local": "wrangler pages dev public --port 8788",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "db:setup:local": "wrangler d1 execute scae_db --local --file=./schema.sql",
    "db:setup:remote": "wrangler d1 execute scae_db --remote --file=./schema.sql --yes"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "firebase": "^12.9.0",
    "html5-qrcode": "^2.3.8",
    "idb": "^8.0.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.563.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.24",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  }
}